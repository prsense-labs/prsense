{
  "name": "prsense",
  "version": "1.0.0",
  "description": "AI-powered PR duplicate detection system using embeddings, bloom filters, and multi-signal scoring",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "demo": "npm run build && node dist/examples/simple-usage.js",
    "cli": "npm run build && node dist/bin/prsense.js",
    "prsense": "npm run build && node dist/bin/prsense-easy.js",
    "server": "npm run build && node dist/src/server.js",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "docker:build": "docker build -t prsense .",
    "docker:run": "docker-compose up",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "bin": {
    "prsense": "./dist/bin/prsense-easy.js"
  },
  "keywords": [
    "pr",
    "pull-request",
    "duplicate-detection",
    "embeddings",
    "bloom-filter",
    "similarity",
    "machine-learning",
    "github",
    "onnx",
    "vector-search",
    "pgvector",
    "sqlite",
    "postgres",
    "mockilo"
  ],
  "author": "PRSense Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prsense-labs/prsense.git"
  },
  "homepage": "https://github.com/prsense-labs/prsense#readme",
  "bugs": {
    "url": "https://github.com/prsense-labs/prsense/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "action.yml"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0",
    "@types/pg": "^8.16.0",
    "@vitest/coverage-v8": "^3.1.0",
    "@vitest/ui": "^3.1.0",
    "typescript": "^5.9.3",
    "vitest": "^3.1.0"
  },
  "optionalDependencies": {
    "better-sqlite3": "^11.0.0",
    "pg": "^8.11.0",
    "onnxruntime-node": "^1.16.0"
  },
  "peerDependencies": {
    "better-sqlite3": "^11.0.0",
    "pg": "^8.11.0"
  },
  "peerDependenciesMeta": {
    "better-sqlite3": {
      "optional": true
    },
    "pg": {
      "optional": true
    }
  }
}